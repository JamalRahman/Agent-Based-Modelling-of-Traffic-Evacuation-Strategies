<?xml version = "1.0"?>

<!-- EXAMPLE 2 -->
<!-- This file can be run from either EvacVisual or ExperimentSequence -->

<!-- This file is INTENDED for visualisation, as it does not have repeats or independent variables-->

<experiment>
    <network type="grid">
        <size>6</size>
        <roadLength>50</roadLength>
    </network>
    
    <repeats>1</repeats>
    <variable var="greedEnabled" type="boolean">true</variable>
    <variable var="throttlingEnabled" type="boolean">true</variable>

    <variable var="populationSize">500</variable>

    <variable var="agentGreedMaxChanges">15</variable>
    <variable var="greedProportion">0.5</variable>
    <variable var="agentGreedThreshold">0.4</variable>

    <variable var="upperThreshold">0.7</variable>
    <variable var="lowerThreshold">0</variable>
</experiment>