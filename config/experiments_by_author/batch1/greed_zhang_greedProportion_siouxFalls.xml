<?xml version = "1.0"?>
<experiment>
    <network type="file">
        <filepath>config/networks/SiouxFallsNet_madireddy.txt</filepath>
    </network>
    <repeats>10</repeats>

    <variable var="populationSize" type="int">1000</variable>
    <variable var="greedProportion" type="double" varType="independent">
        <min>0</min>
        <max>1</max>
        <interval>0.1</interval>
    </variable>
    <variable var="greedEnabled" type="boolean">true</variable>
</experiment>