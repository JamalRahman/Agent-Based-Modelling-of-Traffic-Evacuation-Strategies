<?xml version = "1.0"?>
<experiment>

    <network type="file">
        <filepath>config/networks/SiouxFalls_allSources.net</filepath>
    </network>
    <repeats>1</repeats>
    <timeout>15000</timeout>

    <variable var="populationSize" type="int">1000</variable>
    <variable var="greedEnabled" type="boolean">true</variable>
    <variable var="greedThreshold">0.5</variable>
    <variable var="throttlingEnabled" type="boolean">true</variable>
    <variable var="upperThreshold">0.5</variable>
    <variable var="lowerThreshold">0.3</variable>

</experiment>